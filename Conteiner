package br.com.teste;

public class Conteiners {

	/*
	 * 1.1. Cliente 1.2. Número do contêiner (4 letras e 7 números. Ex: TEST1234567)
	 * 1.3. Tipo: 20 / 40 1.4. Status: Cheio / Vazio 1.5. Categoria: Importação /
	 * Exportação
	 */

	private int Cliente;
	private String Num_Conteiner;
	private String Tipo_Conteiner;
	private String Status;
	private String Categoria;

	public int getCliente() {
		return Cliente;
	}

	public void setCliente(int cliente) {
		Cliente = cliente;
	}

	public String getNum_Conteiner() {
		return Num_Conteiner;
	}

	public void setNum_Conteiner(String num_Conteiner) {
		Num_Conteiner = num_Conteiner;
	}

	public String getTipo_Conteiner() {
		return Tipo_Conteiner;
	}

	public void setTipo_Conteiner(String tipo_Conteiner) {
		Tipo_Conteiner = tipo_Conteiner;
	}

	public String getStatus() {
		return Status;
	}

	public void setStatus(String status) {
		Status = status;
	}

	public String getCategoria() {
		return Categoria;
	}

	public void setCategoria(String categoria) {
		Categoria = categoria;
	}
}
